{{define "title"}}Ding Details{{end}}
{{define "content"}}
<article>
  <!-- <img src="https://picsum.photos/800/450?random{{.FormValues.Id}}" alt=""> -->
  <img src="{{.FormValues.PhotoUrl}}" alt="">
  <h4>{{.FormValues.Name}}<sup>{{.FormValues.Anzahl}}</sup></h4>
  <p>{{.FormValues.Code}}</p>
  <p>{{.FormValues.Anzahl}}</p>

  <pre>{{.FormValues.Beschreibung}}</pre>

  <nav>
    <a href="/dinge/{{.FormValues.Id}}/edit">Bearbeiten</a>
    <a href="/dinge/{{.FormValues.Id}}/photo">Foto Bearbeiten</a>
    <a href="/dinge/{{.FormValues.Id}}">Einlagern</a>
    <a href="/dinge/{{.FormValues.Id}}">Entnehmen</a>
  </nav>
</article>
{{end}}
