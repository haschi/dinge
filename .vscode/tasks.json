{
	"version": "2.0.0",
	"type": "shell",
	"tasks": [
		{
			"label": "go: build package",
			"command": "go",
			"args": [
				"build",
				"${workspaceFolder}",
				"tags",
				"fts5"
			],
			"problemMatcher": [
				"$go"
			],
			"group": "build",
			"detail": "cd /workspaces/dinge; go build ${workspaceFolder}"
		},
		{
			"label": "go: run package",
			"command": "go",
			"args": [
				"tags",
				"fts5",
				"run",
				"${workspaceFolder}"
			],
			"problemMatcher": [
				"$go"
			],
			"group": "build",
			"detail": "cd /workspaces/dinge; go run ${workspaceFolder}"
		}
	]
}
